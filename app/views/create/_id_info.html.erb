<%= form_for @qtest, url: create_path, html: {class: 'form-horizontal'} do |f| %>
  <% if @qtest.errors.any? %>
    <div id='error_explanation'>
      <h2><%= pluralize(@qtest.errors.count, 'error') %>
        prohibited this form from being saved:
      </h2>
      <ul>
        <% @qtest.errors.full_messages.each do |msg| %>
          <li class='alert fade in alert-danger alert-dismissable'>
            <%= msg %>
          </li>
        <% end %>
      </ul>
    </div>
  <% end %>
  <div class='control-group'>
    <%= f.label :coach_first_name, class: 'control-label' %>
    <div class='controls'>
      <%= f.text_field :coach_first_name, class: 'text_field' %>
    </div>
  </div>
  <div class='control-group'>
    <%= f.label :coach_last_name, class: 'control-label' %>
    <div class='controls'>
      <%= f.text_field :coach_last_name, class:'text_field' %>
    </div>
  </div>
  <div class='control-group'>
    <%= f.label :coach_email, class: 'control-label' %>
    <div class='controls'>
      <%= f.email_field :coach_email, class: 'email_field' %>
    </div>
  </div>
  <div class='control-group'>
    <%= f.label :company, class: 'control-label' %>
    <div class='controls'>
      <%= f.text_field :company, class: 'text_field' %>
    </div>
  </div>
  <div class='control-group'>
    <%= f.label :license_quantity, class: 'control-label' %>
    <div class='controls'>
      <%= f.number_field :license_quantity,
                         value: nil,
                         min: 1,
                         class: 'number_field' %>
    </div>
  </div>
  <div class='control-group'>
    <%= f.label :multi_license, class: 'control-label' %>
    <div class='controls'>
      <%= f.check_box :multi_license, class: 'checkbox' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :send_to_client, class: 'control-label' %>
    <div class='controls'>
      <%= f.check_box :send_to_client, class: 'checkbox' %>
    </div>
  </div>
  <div class='control-group'>
    <%= f.label :password, class: 'control-label' %>
    <div class='controls'>
      <%= f.password_field :password, class: 'text_field' %>
    </div>
  </div>
  <div class='form-actions'>
    <%= f.submit 'Create Licenses', class: 'btn btn-primary' %>
  </div>
<% end %>
