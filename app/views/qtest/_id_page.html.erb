<%= form_for @qtest, url: qtest_path, :html => {:class => 'form-horizontal'} do |f| %>
<% if @qtest.errors.any? %>
  <div id="error_explanation">
    <h2><%= pluralize(@qtest.errors.count, "error") %> prohibited this form from being
      saved:</h2>
    <ul>
      <% @qtest.errors.full_messages.each do |msg| %>
        <li class='alert fade in alert-danger alert-dismissable'><%= msg %></li>
      <% end %>
    </ul>
  </div>
<% end %>
<div class="control-group">
  <%= f.label :first_name, :class => 'control-label' %>
  <div class="controls">
    <%= f.text_field :first_name, :class => 'text_field' %>
  </div>
</div>
<div class="control-group">
  <%= f.label :last_name, :class => 'control-label' %>
  <div class="controls">
    <%= f.text_field :last_name, :class => 'text_field' %>
  </div>
</div>
<div class="control-group">
  <%= f.label :email, :class => 'control-label' %>
  <div class="controls">
    <%= f.email_field :email,  :class => 'text_field' %>
  </div>
</div>

<div class="form-actions">
  <%= f.submit 'Start Assessment', :class => 'btn btn-primary' %>
</div>
<% end %>
